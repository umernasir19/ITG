<%@ Page Title="Accounts Tree" Language="vb" AutoEventWireup="false" MasterPageFile="~/MasterPage.master" CodeBehind="AccountsTree.aspx.vb" Inherits="Integra.AccountsTree" %>
<%@ Register Assembly="System.Web.Extensions, Version=1.0.61025.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35"
    Namespace="System.Web.UI" TagPrefix="asp" %>
<%@ Register TagPrefix="Smart" Namespace="SmartControls.Controls" Assembly="SmartControls" %>
<%@ Register Assembly="AjaxControlToolkit" Namespace="AjaxControlToolkit" TagPrefix="cc1" %>
<asp:Content ID="Content1" ContentPlaceHolderID="ContentPlaceHolder1" runat="server">
<table>
        <tr>
            <td>
                <asp:Button ID="btnManage" CssClass="textbox" runat="server" Text="Tree Collapse" />
                <asp:Button ID="btnExpand" CssClass="textbox" runat="server"  Visible ="false" Text="Tree Expand" />
                <asp:Button ID="btnBack" CssClass="textbox" runat="server" Text="Back" /></td>
               
        </tr>
        <tr>
            <td>
                <asp:TreeView ID="Tree" runat="server" AutoGenerateDataBindings="false" ExpandDepth="0"
                    Style="width: 930px; height: auto;" BorderStyle="Ridge" ImageSet="BulletedList3"
                    MaxDataBindDepth="0" ShowLines="True">
                    <LevelStyles>
                        <asp:TreeNodeStyle Font-Bold="True" Font-Underline="False" ForeColor="Maroon" />
                        <asp:TreeNodeStyle Font-Bold="true" ForeColor="Navy" Font-Underline="False" />
                        <asp:TreeNodeStyle Font-Bold="true" Font-Underline="False" ForeColor="green" />
                        <asp:TreeNodeStyle Font-Bold="true" Font-Underline="False" ForeColor="Gray" />
                        <asp:TreeNodeStyle Font-Bold="true" Font-Underline="False" ForeColor="Black" />
                    </LevelStyles>
                </asp:TreeView>
            </td>
        </tr>
    </table>
</asp:Content>

